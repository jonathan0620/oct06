package 스태틱;

public class 직원 {
	public String name;
	public int age;
	public String mf;
	public static int count;
	public static int sum;
	
	public 직원(String name, int age, String mf) {
		super();
		this.name = name;
		this.age = age;
		this.mf = mf;
		count++;
		sum = sum + age;
	}
	@Override
	public String toString() {
		return "직원 [name=" + name + ", age=" + age + ", mf=" + mf + "]";
	}
	
	
	
}
